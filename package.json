{
    "name": "Hello-Books-BE",
    "version": "1.0.0",
    "description": "A system that allows efficient, cost-effective management of our library",
    "main": "index.js",
    "scripts": {
        "start": "node server/index.js",
        "start:dev": "nodemon --exec babel-node server/index.js",
        "test": "nyc mocha --timeout 10000 -r esm --exit",
        "generate-lcov": "nyc report --reporter=text-lcov > lcov.info",
        "db:rollback": "npm run db:back && npm run db:roll && npm run db:seed",
        "db:roll": "sequelize db:migrate",
        "db:back": "sequelize db:migrate:undo:all",
        "db:seed": "sequelize db:seed:all",
        "coverage": "npm run generate-lcov"
    },
    "repository": "git@github.com:KvNGCzA/Hello-Books-BE.git",
    "author": "KvNGCzA <akanmuoluwadamilare@yahoo.com>",
    "license": "MIT",
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^8.0.0",
        "express": "^4.17.1",
        "express-validator": "^5.3.1",
        "morgan": "^1.9.1",
        "pg": "^7.11.0",
        "pg-hstore": "^2.3.2",
        "swagger-ui-express": "^4.0.5",
        "sequelize": "^5.8.6"
    },
    "devDependencies": {
        "@babel/cli": "^7.4.4",
        "@babel/core": "^7.4.5",
        "@babel/node": "^7.4.5",
        "@babel/preset-env": "^7.4.5",
        "@babel/register": "^7.4.4",
        "nodemon": "^1.19.1",
        "chai": "^4.2.0",
        "chai-http": "^4.3.0",
        "esm": "^3.2.25",
        "mocha": "^6.1.4",
        "nyc": "^14.1.1",
        "mocha-lcov-reporter": "^1.3.0",
        "eslint": "^5.16.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-plugin-import": "^2.17.3",
        "sequelize-cli": "^5.4.0"
    }
}
